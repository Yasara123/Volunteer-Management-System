<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts 
PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="default" extends="struts-default">
	
	<action name="movie_list" class="com.im.struts.actions.MovieAction">
		<result name="success">/movies.jsp</result>
		<result name="error">/error.jsp</result>
	</action>
	<action name="audio_list" class="com.im.struts.actions.AudioAction">
		<result name="success">/audios.jsp</result>
		<result name="error">/error.jsp</result>
	</action>
	<action name="photo_lis" class="com.im.struts.actions.PhotoAction">
		<result name="success">/photos.jsp</result>
		<result name="error">/error.jsp</result>
	</action>
	
	<action name="reg" class="com.im.struts.actions.RegisterAction">
		<result name="success">/Thankyou.jsp</result>
		<result name="error">/error.jsp</result>
	</action>
	<action name="log" class="com.im.struts.actions.LoginAction">
		<result name="success">/welcome.jsp</result>
		<result name="error">/error.jsp</result>
		<result name="input">/login.jsp</result>
	</action>
	<action name="search" class="com.im.struts.actions.SearchAction" method="searchMovie">
		<result name="success">/movies.jsp</result>
		<result name="error">/error.jsp</result>
	</action>
	</package>
</struts> 